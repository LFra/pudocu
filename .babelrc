{
	"presets": [
		"next/babel",
		"stage-0"
	],
	"plugins": [
		["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ],
		["module-resolver", {
			"root": ["./"],
			"alias": {
				"components": "./components",
				"container": "./container"
			}
		}]
	],
	"env": {
		"development": {
			"presets": "next/babel"
		},
		"production": {
			"presets": "next/babel"
		},
		"test": {
			"presets": [
				["env", { "modules": "commonjs" }],
				"next/babel"
			]
		}
	}
}